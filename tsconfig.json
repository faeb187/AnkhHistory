{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": "./src",
    "esModuleInterop": true,
    "lib": ["dom","es6"],
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "./dist",
    "paths": {
      "app": ["app"],
      "core": ["core"],
      "styl": ["styl"],
      "types": ["types"],
      "uis": ["uis"],
      "utils": ["utils"]
    },
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "./src",
    "sourceMap": true,
    "strict": true,
    "target": "es6",
  },
}