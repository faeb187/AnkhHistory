{
  "name": "ankh",
  "version": "0.0.1",
  "description": "modern lightweight and flexible UI/UX framework.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faeb187/ankh.git"
  },
  "keywords": [
    "UI",
    "UX",
    "framework",
    "mobile first",
    "content first",
    "coffee",
    "html5",
    "css3"
  ],
  "author": "faeb187",
  "license": "MIT",
  "test": "jest --verbose --forceExit --coverage --detectOpenHandles --passWithNoTests",
  "jest": {
    "preset": "ts-jest",
    "modulePathIgnorePatterns": [
      "dist"
    ]
  },
  "watch": {
    "start": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "coffee",
        "styl",
        "pug"
      ]
    }
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "gql": "node ../graphql-server.js",
    "start": "./start.sh",
    "watch": "npm-watch"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "browsernizr": "^2.3.0",
    "graphql": "^15.3.0",
    "hammerjs": "^2.0.8",
    "hotkeys": "^1.0.0",
    "material-colors": "^1.2.6",
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/jest": "^26.0.15",
    "coffee-loader": "^1.0.1",
    "coffeescript": "^2.5.1",
    "graphql-tag": "^2.11.0",
    "gulp": "^4.0.2",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-rucksack": "^0.1.4",
    "gulp-stylus": "^2.7.0",
    "jest": "^26.6.0",
    "npm-watch": "^0.7.0",
    "poststylus": "^1.0.1",
    "prettier": "github:helixbass/prettier#prettier-v2.1.0-dev.100-gitpkg",
    "prettier-plugin-coffeescript": "^0.1.5",
    "rupture": "^0.7.1",
    "ts-jest": "^26.4.1",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0"
  }
}
